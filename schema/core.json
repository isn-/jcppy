{
    "name": "Person",
    "title": "A typical person",
    "description": "This is a long description of person class\nThis desctiption could be multilined",
    "type": "object",
    "properties": {
        "age": {
            "type": "integer",
            "maximum": 125,
            "minimum": 0
        },
        "name": {
            "type": "string"
        },
        "gender": {
            "type": "boolean",
            "description": "true stands for female"
        },
        "married": {
            "type": "boolean",
            "title": "Married or not?",
            "default": true
        },
        "wealth": {
            "type": "number",
            "default": "1000000.2"
        },
        "dateOfBirth": {
            "type": "string",
            "format": "date-time"
        },
        "religion": {
            "type": "string",
            "enum": ["christianity", "islam", "hinduism", "buddism"]
        },
        "dateOfBirth2": {
            "type": "string",
            "format": "utc-millisec"
        },
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "photo": {
            "type": "string",
            "format": "base64"
        }
    },
    "required": ["age", "gender", "dateOfBirth"]
}
